{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Octave Debugger: Current File",
            "type": "OctaveDebugger",
            "request": "launch",
            "program": "${file}",
            "octave": "octave-cli",
            "sourceFolder": "${workspaceFolder}"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}
